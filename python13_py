# 리스트를 선언합니다.
list_a = [1, 2, 3]
list_b = [4, 5, 6]

print("리스트")
print("list_a:", list_a)
print("list_b:", list_b)

#리스트 기본 연산자인 연결과 반복을 실행합니다.
print("리스트 기본 연산자")
print("list_a + list_b:", list_a + list_b)
print("list_a * 3:", list_a * 3)

#리스트의 길이를 구합니다.
print("리스트의 길이")
print("list_a * 5:", len(list_a * 5))