print(type(52))
print(type(52.27))
print(0.52273e2) #e2는 10^2을 의미한다.
print("5 + 7 =", 5 + 7)
print("5 - 7 =", 5 - 7)
print("5 * 7 =", 5 * 7)
print("5 / 7 =", 5 / 7)
print("5 // 7 =", 5 // 7) #//는 정수부분만 남기는 정수 나누기 연산자이다.
print("5 % 2 =", 5 % 2) #연산자 %는 나머지를 구하는 나머지 연산자이다.
print("2 ** 4 =", 2 ** 4) # 연산자 **는 제곱 연산자이다.