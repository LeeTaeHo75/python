score = float(input("성적을 입력하세요: "))

if score == 4.5:
    print("성적은 A+입니다")

elif 4 <= score < 4.5:
    print("성적은 A0입니다")
elif 3.5 <= score < 4:
    print("성적은 b+입니다")
elif 3 <= score < 3.5:
    print("성적은 B0입니다")
elif 2.5 <= score < 3:
    print("성적은 C+입니다")
elif 2 <= score < 2.5:
    print("성적은 C0입니다")
elif 1.5 <= score < 2:
    print("성적은 D+입니다")
elif score < 1.5:
    print("재수강하세요")